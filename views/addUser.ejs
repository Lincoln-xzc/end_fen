<%include header.ejs%>
<!-- BEGIN CONTAINER -->
<div class="page-container">
    <%include menu.ejs%>
    <!-- BEGIN PAGE -->
    <div class="page-content">
        <!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
        <div id="portlet-config" class="modal hide">
            <div class="modal-header">
                <button data-dismiss="modal" class="close" type="button"></button>
                <h3>Widget Settings</h3>
            </div>
            <div class="modal-body">
                Widget settings form goes here
            </div>
        </div>
        <!-- END SAMPLE PORTLET CONFIGURATION MODAL FORM-->
        <!-- BEGIN PAGE CONTAINER-->
        <div class="container-fluid">
            <!-- BEGIN PAGE HEADER-->
            <div class="row-fluid">
                <div class="span12">

                    <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                    <h3 class="page-title">
                        用户中心 <small>statistics and more</small>
                    </h3>
                    <ul class="breadcrumb">
                        <li>
                            <i class="icon-home"></i>
                            <a href="/api/users/addUser">新增用户</a>
                            <i class="icon-angle-right"></i>
                        </li>
                        <li><a href="#">addUser</a></li>
                    </ul>
                    <!-- END PAGE TITLE & BREADCRUMB-->
                </div>
            </div>
            <!-- END PAGE HEADER-->
            <div id="dashboard">
                <form action="/api/users/register" method="post" class="form-horizontal">
                    <div class="control-group">
                        <label class="control-label" style="margin-right: 10px;">用户名</label>
                        <input type="text" name="username" placeholder="请输入用户名" class="m-wrap medium">
                        <span class="help-inline"></span>
                    </div>
                    <div class="control-group">
                        <label class="control-label" style="margin-right: 10px;">密码</label>
                        <input type="text" name="password" placeholder="请输入密码" class="m-wrap medium">
                    </div>
                    <div class="form-actions">
                        <button class="btn blue" type="submit">添加</button>
                    </div>
                </form>
            </div>
        </div>
        <!-- END PAGE CONTAINER-->
    </div>
    <!-- END PAGE -->
</div>

<%include footer.ejs%>